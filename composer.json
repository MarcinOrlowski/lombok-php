{
  "name": "marcin-orlowski/lombok-php",
  "description": "Never write boilerplate code for your data class again!",
  "homepage": "https://github.com/MarcinOrlowski/lombok-php",
  "version": "1.2.0",
  "keywords": [
    "lombok",
    "annotation",
    "annotations",
    "getters",
    "setters",
    "accessors",
    "php",
    "php8"
  ],
  "license": "LGPL-3.0-only",
  "authors": [
    {
      "name": "Marcin Orlowski",
      "homepage": "https://MarcinOrlowski.com/"
    }
  ],
  "support": {
    "source": "https://github.com/MarcinOrlowski/lombok-php/",
    "issues": "https://github.com/MarcinOrlowski/lombok-php/issues"
  },
  "minimum-stability": "stable",
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3|^8.4|^8.5"
  },
  "autoload": {
    "psr-4": {
      "Lombok\\": "src/",
      "LombokTest\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.0",
    "phpstan/phpstan": "^2.0"
  },
  "scripts": {
    "test": [
      "Composer\\Config::disableProcessTimeout",
      "vendor/bin/phpunit -c phpunit.xml -c phpunit.xml"
    ],
    "phpstan": [
      "Composer\\Config::disableProcessTimeout",
      "vendor/bin/phpstan analyze -c phpstan.neon --no-progress --no-interaction src/ tests/"
    ],
    "lint": [
      "Composer\\Config::disableProcessTimeout",
      "@composer phpstan"
    ],
    "mdlint": [
      "Composer\\Config::disableProcessTimeout",
      "markdownlint --config .markdownlint.yaml --ignore vendor --ignore LICENSE.md **/*.md .github/**/*.md docs README.md"
    ]
  }
}
